{
  "private": true,
  "name": "invokation",
  "version": "dev",
  "type": "commonjs",
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=14.16"
  },
  "dota2": {
    "customGame": {
      "name": "invokation"
    }
  },
  "scripts": {
    "clean": "node scripts/main.js clean",
    "link": "node scripts/main.js link",
    "build:content": "node scripts/main.js build-content",
    "build:game": "node scripts/main.js build-game",
    "format:lua": "node scripts/main.js format-lua",
    "format:panorama": "run format:panorama:prettier",
    "format:panorama:stylelint:fix": "stylelint --fix content/panorama/**/*.{css,scss,vue}",
    "format:panorama:eslint:fix": "eslint --fix content/panorama/**/*.{js,ts,vue}",
    "format:panorama:prettier": "prettier --write content/panorama/**/*.{css,scss,js,ts,vue}",
    "format:tasks": "prettier --write scripts/**/*.{js,ts}",
    "lint:panorama": "run lint:panorama:eslint && run lint:panorama:stylelint",
    "lint:panorama:eslint": "eslint --ext .js,.ts,.vue content/panorama",
    "lint:panorama:stylelint": "stylelint content/panorama/**/*.{css,scss,vue}",
    "lint:tasks": "eslint --ext .js,.ts scripts",
    "launch:game": "node scripts/main.js launch game",
    "launch:tools": "node scripts/main.js launch tools",
    "test:panorama": "mocha"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@moddota/dota-data": "^0.17.0",
    "@moddota/panorama-types": "^1.9.0",
    "@node-steam/vdf": "^2.2.0",
    "@types/chai": "^4.2.16",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.41",
    "@types/postcss-url": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/runtime-core": "^3.0.11",
    "chai": "^4.3.4",
    "chalk": "^4.1.0",
    "commander": "^7.2.0",
    "css-loader": "^5.2.4",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^7.9.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.5",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.3",
    "js-yaml": "^4.1.0",
    "kv3tojs": "^0.0.2",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha": "^8.3.2",
    "node-emoji": "^1.10.0",
    "p-map": "^4.0.0",
    "picomatch": "^2.2.3",
    "postcss": "^8.2.15",
    "postcss-html": "^0.36.0",
    "postcss-safe-parser": "^5.0.2",
    "postcss-syntax": "^0.36.2",
    "postcss-url": "^10.1.3",
    "postcss-value-parser": "^4.1.0",
    "prettier": "^2.3.0",
    "pretty-ms": "^7.0.1",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "tmp-promise": "^3.0.2",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vue": "^3.2.37",
    "vue-eslint-parser": "^7.6.0",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
